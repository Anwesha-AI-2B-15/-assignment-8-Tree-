'''
# Node Class:
class Node:
    def _init_(self,val):
        self.data = val
        self.left = None
        self.right = None
'''
#Complete the function below
class Solution:
    def diagonal(self,root):
        #:param root: root of the given tree.
        #return: print out the diagonal traversal,  no need to print new line
        #code here
        ans = []
        q = [root]
        while q:
            __ = []
            for _ in range(len(q)):
                top = q.pop(0)
                while top:
                    ans.append(top.data)
                    if top.left:
                        __.append(top.left)
                    top = top.right
            q = __
        return ans
