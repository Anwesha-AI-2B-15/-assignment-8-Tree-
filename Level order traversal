"""
class Node:
    def __init__(self, value):
        self.left = None
        self.data = value
        self.right = None
"""
class Solution:
    def levelOrder(self, root):
        # Your code here
        q = [root]
        res = []
        while q:
            tmp = []
            l = len(q)
            for i in range(l):
                n = q.pop(0)
                tmp.append( n.data )
                if n.left:
                    q.append(n.left)
                if n.right:
                    q.append(n.right)
            res.append(tmp)
        return res
