'''
class Node:
    def _init_(self, val):
        self.right = None
        self.data = val
        self.left = None
'''
class Solution:
    def mirror(self, root):
        # Code here
        if not root:
           return
        root.left,root.right = root.right,root.left
        self.mirror(root.left)
        self.mirror(root.right)
